#steps taken in adm0 project for UNIX
cd ..
cd ..
cd var
cd public
cd spring2017
cd unix
cd adm0
cd ccattell
cp adm0a.zip ~/src/unix/projects/adm0
cp adm0b.tar.gz ~/src/unix/projects/adm0
cp adm0c.tar.bz2 ~/src/unix/projects/adm0
cp adm0d.tar.xz ~/src/unix/projects/adm0 
cd
cd src
cd unix
cd projects
cd adm0
unzip adm0a.zip
tar -xvzf adm0b.tar.gz
tar -xvjf adm0c.tar.bz2
tar -xvf adm0d.tar.xz
unzip 1.zip
#nano all files to see what they contain
#out5 not readable - FORTRAN program, ASCII text
#out62 not readable - uuencoded or xxencoded, ASCII text
#out72 not readable - uuencodede or xxencoded, ASCII text
#out8 not readable - uuencoded or xxencoded, ASCII text
rev out5 > 5
#reveresed out5, created a file of that output and called it 5
uudecode -o 62decode out62
#created unencoded version of out62 > 62
tac 62decode > 62
#unflip 62...
uudecode -o 72decode out72
rev 72decode > 72
uudecode -o 8 out8
tac out3 > 3
cp 1 a
cp 2 b
cp 3 c
cp 4 d
cp 5 e
cp 62 f
cp 72 g
cp 8 h
tar -cvf adm0e.tar a b c d e f g h
gzip -9 adm0e.tar

