#cat h2rev | cut -c -34 > q2
#cat h2rev | cut -c 44- > q1
#paste -dX q3 q4 > bottomhalf
#paste -dX q1 q2 > tophalf
#cat tophalf bottomhalf > wholeX
#cat wholeX | tr -d X > whole

